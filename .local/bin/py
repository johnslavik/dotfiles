#!/usr/bin/env bash
if [[ -e "./python.exe" ]] && \
  [[ "$(git remote get-url upstream)" == "git@github.com:python/cpython.git" ]]; then
  ./python.exe "$@"
elif [[ -e "./python" ]] && \
  [[ "$(git remote get-url upstream)" == "git@github.com:python/cpython.git" ]]; then
  ./python "$@"
else
  python "$@"
fi
